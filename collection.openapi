openapi: 3.0.0
info:
  title: FlyNext API
  version: 1.0.0
servers:
  - url: localhost://
  - url: http://localhost:3000
components:
  securitySchemes:
    bearerAuth:
      type: http
      scheme: bearer
security:
  - bearerAuth: []
tags:
  - name: Hotel Management and Search
  - name: Notifications
    description: >-
      Methods to create, update, and retrieve notifications for a user in the
      fly next backend
  - name: Flights
    description: >-
      Demonstrates the variations of the flight search for fly next, which
      requires an origin, destination corresponding to an airport or city.
      Requires two validate dates for round trip and one for one way, formatted
      'YYYY-MM-DD'.
  - name: Booking
    description: >-
      Can create, cancel, and verify flight and/or hotel bookings. Needs booking
      IDs for cancellation and verification.
  - name: Auth
    description: >-
      Uses JWT tokens for authentication and stores user ID to validate
      throughout the app.
paths:
  /000/api/hotels:
    post:
      tags:
        - Hotel Management and Search
      summary: Create Hotel
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                name: Example Hotel
                logo: https://example.com/logo.png
                address: 123 Example Street
                location: Mississauga, ON
                city: Mississauga
                starRating: 5
                images:
                  - https://example.com/image1.jpg
                  - https://example.com/image2.jpg
      parameters:
        - name: Authorization
          in: header
          schema:
            type: string
          example: >-
            Bearer
            eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiJlNWM4MmVkYy1jYzAyLTQ5MjQtOTczMS04NWQ3NDc4NWMyNGYiLCJlbWFpbCI6ImpvaG5kb2VAZXhhbXBsZS5jb20iLCJyb2xlIjoiVVNFUiIsImlhdCI6MTc0MTY2NTcyMiwiZXhwIjoxNzQxNzUyMTIyfQ.gfBXyd-dsY1Q5tGIPON9vJtc5C0xL9MYNdGaoWeSHbI
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
    get:
      tags:
        - Hotel Management and Search
      summary: Search and Filter Hotels
      parameters:
        - name: checkIn
          in: query
          schema:
            type: string
          example: '2025-03-15'
        - name: checkOut
          in: query
          schema:
            type: string
          example: '2025-03-22'
        - name: city
          in: query
          schema:
            type: string
          example: Toronto
        - name: name
          in: query
          schema:
            type: string
          example: Example Hotel
        - name: starRating
          in: query
          schema:
            type: integer
          example: '5'
        - name: priceRange
          in: query
          schema:
            type: string
          example: 200-300
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /000/api/hotels/[id]:
    get:
      tags:
        - Hotel Management and Search
      summary: View Hotel Info
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /000/api/hotels/[id]/rooms:
    post:
      tags:
        - Hotel Management and Search
      summary: Define Room Types
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                name: Deluxe Suite
                amenities:
                  - Wi-Fi
                  - Air Conditioning
                  - Flat-screen TV
                  - Mini Bar
                  - Jacuzzi
                pricePerNight: 250
                images:
                  - https://example.com/images/room1.jpg
                  - https://example.com/images/room2.jpg
                  - https://example.com/images/room3.jpg
                availableRooms: 5
      parameters:
        - name: Authorization
          in: header
          schema:
            type: string
          example: >-
            Bearer
            eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiJlNWM4MmVkYy1jYzAyLTQ5MjQtOTczMS04NWQ3NDc4NWMyNGYiLCJlbWFpbCI6ImpvaG5kb2VAZXhhbXBsZS5jb20iLCJyb2xlIjoiVVNFUiIsImlhdCI6MTc0MTY2NTcyMiwiZXhwIjoxNzQxNzUyMTIyfQ.gfBXyd-dsY1Q5tGIPON9vJtc5C0xL9MYNdGaoWeSHbI
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
    put:
      tags:
        - Hotel Management and Search
      summary: Update Number of Available Rooms
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                availableRooms: 3
      parameters:
        - name: Authorization
          in: header
          schema:
            type: string
          example: >-
            Bearer
            eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiJlNWM4MmVkYy1jYzAyLTQ5MjQtOTczMS04NWQ3NDc4NWMyNGYiLCJlbWFpbCI6ImpvaG5kb2VAZXhhbXBsZS5jb20iLCJyb2xlIjoiVVNFUiIsImlhdCI6MTc0MTY2NTcyMiwiZXhwIjoxNzQxNzUyMTIyfQ.gfBXyd-dsY1Q5tGIPON9vJtc5C0xL9MYNdGaoWeSHbI
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /000/api/hotels/[id]/bookings:
    get:
      tags:
        - Hotel Management and Search
      summary: View and Filter Hotel Bookings
      parameters:
        - name: Authorization
          in: header
          schema:
            type: string
          example: >-
            Bearer
            eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiJlNWM4MmVkYy1jYzAyLTQ5MjQtOTczMS04NWQ3NDc4NWMyNGYiLCJlbWFpbCI6ImpvaG5kb2VAZXhhbXBsZS5jb20iLCJyb2xlIjoiVVNFUiIsImlhdCI6MTc0MTY2NTcyMiwiZXhwIjoxNzQxNzUyMTIyfQ.gfBXyd-dsY1Q5tGIPON9vJtc5C0xL9MYNdGaoWeSHbI
        - name: date
          in: query
          schema:
            type: string
          example: '2025-03-02'
        - name: roomType
          in: query
          schema:
            type: string
          example: Deluxe Suite
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
    put:
      tags:
        - Hotel Management and Search
      summary: Cancel Hotel Booking
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                bookingId: abc123
      parameters:
        - name: Authorization
          in: header
          schema:
            type: string
          example: >-
            Bearer
            eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiJlNWM4MmVkYy1jYzAyLTQ5MjQtOTczMS04NWQ3NDc4NWMyNGYiLCJlbWFpbCI6ImpvaG5kb2VAZXhhbXBsZS5jb20iLCJyb2xlIjoiVVNFUiIsImlhdCI6MTc0MTY2NTcyMiwiZXhwIjoxNzQxNzUyMTIyfQ.gfBXyd-dsY1Q5tGIPON9vJtc5C0xL9MYNdGaoWeSHbI
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /000/api/hotels/[id]/availability:
    get:
      tags:
        - Hotel Management and Search
      summary: View Room Availability
      parameters:
        - name: Authorization
          in: header
          schema:
            type: string
          example: >-
            Bearer
            eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VySWQiOiJlNWM4MmVkYy1jYzAyLTQ5MjQtOTczMS04NWQ3NDc4NWMyNGYiLCJlbWFpbCI6ImpvaG5kb2VAZXhhbXBsZS5jb20iLCJyb2xlIjoiVVNFUiIsImlhdCI6MTc0MTY2NTcyMiwiZXhwIjoxNzQxNzUyMTIyfQ.gfBXyd-dsY1Q5tGIPON9vJtc5C0xL9MYNdGaoWeSHbI
        - name: startDate
          in: query
          schema:
            type: string
          example: '2025-03-15'
        - name: endDate
          in: query
          schema:
            type: string
          example: '2025-03-20'
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/notifications:
    post:
      tags:
        - Notifications
      summary: Create Notification
      description: Creates notification for a user
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                message: Booking Confirmed
                uid: cfc718bc-87c0-4290-8b52-04ce4da9b253
      responses:
        '200':
          description: OK
          headers:
            vary:
              schema:
                type: string
                example: >-
                  RSC, Next-Router-State-Tree, Next-Router-Prefetch,
                  Next-Router-Segment-Prefetch
            content-type:
              schema:
                type: string
                example: application/json
            Date:
              schema:
                type: string
                example: Tue, 11 Mar 2025 17:29:58 GMT
            Connection:
              schema:
                type: string
                example: keep-alive
            Keep-Alive:
              schema:
                type: string
                example: timeout=5
            Transfer-Encoding:
              schema:
                type: string
                example: chunked
          content:
            application/json:
              schema:
                type: object
              example:
                id: 1bc5482a-73f8-470f-92d7-cc5b988fa750
                message: Booking Confirmed
                isRead: false
                createdAt: '2025-03-11T17:29:58.904Z'
                userId: cfc718bc-87c0-4290-8b52-04ce4da9b253
    get:
      tags:
        - Notifications
      summary: View Unread Notifications
      description: >-
        Get all notifications for a user, along with their unread notification
        count
      parameters:
        - name: uid
          in: query
          schema:
            type: string
          description: The id of the user whose notifications we would like to retrieve
          example: cfc718bc-87c0-4290-8b52-04ce4da9b253
      responses:
        '200':
          description: OK
          headers:
            vary:
              schema:
                type: string
                example: >-
                  RSC, Next-Router-State-Tree, Next-Router-Prefetch,
                  Next-Router-Segment-Prefetch
            content-type:
              schema:
                type: string
                example: application/json
            Date:
              schema:
                type: string
                example: Tue, 11 Mar 2025 23:49:35 GMT
            Connection:
              schema:
                type: string
                example: keep-alive
            Keep-Alive:
              schema:
                type: string
                example: timeout=5
            Transfer-Encoding:
              schema:
                type: string
                example: chunked
          content:
            application/json:
              schema:
                type: object
              example:
                notifications:
                  - id: 1bc5482a-73f8-470f-92d7-cc5b988fa750
                    message: Booking Confirmed
                    isRead: true
                    createdAt: '2025-03-11T17:29:58.904Z'
                    userId: cfc718bc-87c0-4290-8b52-04ce4da9b253
                  - id: 7d75a977-6632-47dd-83c8-a3eaa58cb225
                    message: Booking Confirmed
                    isRead: false
                    createdAt: '2025-03-11T23:09:08.393Z'
                    userId: cfc718bc-87c0-4290-8b52-04ce4da9b253
                unreadCount: 1
    patch:
      tags:
        - Notifications
      summary: Updating Notifications
      description: Updates the read status of a notification
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                id: 1bc5482a-73f8-470f-92d7-cc5b988fa750
                isRead: false
      responses:
        '200':
          description: OK
          headers:
            vary:
              schema:
                type: string
                example: >-
                  RSC, Next-Router-State-Tree, Next-Router-Prefetch,
                  Next-Router-Segment-Prefetch
            content-type:
              schema:
                type: string
                example: application/json
            Date:
              schema:
                type: string
                example: Wed, 12 Mar 2025 00:08:00 GMT
            Connection:
              schema:
                type: string
                example: keep-alive
            Keep-Alive:
              schema:
                type: string
                example: timeout=5
            Transfer-Encoding:
              schema:
                type: string
                example: chunked
          content:
            application/json:
              schema:
                type: object
              example:
                id: 1bc5482a-73f8-470f-92d7-cc5b988fa750
                message: Booking Confirmed
                isRead: false
                createdAt: '2025-03-11T17:29:58.904Z'
                userId: cfc718bc-87c0-4290-8b52-04ce4da9b253
