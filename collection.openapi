  /api/booking/itinerary:
    post:
      tags:
        - Booking
      summary: itinerary
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                flightIds:
                  - b83fea74-01cd-418e-b09d-c761f8206573
                passportNumber: '123239934'
                checkIn: '2025-03-11'
                checkOut: '2025-03-21'
                roomTypeId: ba342ec4-f557-4b0f-b78e-93fa19e4e7d5
                hotelId: edbb90fb-2266-45bf-bc17-80c629b62e85
      security:
        - bearerAuth: []
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/booking/cancel:
    post:
      tags:
        - Booking
      summary: cancel
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                bookingId: 83a993f8-834b-4104-b5ad-9c4c4a2b7c09
                flightBookingId: 3e8d2fd2-993c-4387-bf20-65edb076df8c
                hotelBookingId: 2581ac90-63ba-4eee-9872-d823a9f7bbb6
      security:
        - bearerAuth: []
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/booking/invoice/87724364-f2a2-4019-ae84-05b6dc1df853:
    post:
      tags:
        - Booking
      summary: invoice
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                bookingId: 83a993f8-834b-4104-b5ad-9c4c4a2b7c09
                flightBookingId: 3e8d2fd2-993c-4387-bf20-65edb076df8c
                hotelBookingId: 2581ac90-63ba-4eee-9872-d823a9f7bbb6
      security:
        - bearerAuth: []
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/booking/verify:
    post:
      tags:
        - Booking
      summary: verify
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                flightId: b83fea74-01cd-418e-b09d-c761f8206573
                passportNumber: '123239934'
                checkIn: '2025-03-11'
                checkOut: '2025-03-21'
      security:
        - bearerAuth: []
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/booking/checkout:
    post:
      tags:
        - Booking
      summary: checkout
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                bookingId: 83a993f8-834b-4104-b5ad-9c4c4a2b7c09
                cardNumber: '12445567889978'
                expiry: 11/27
                nameOnCard: Jane Doe
      security:
        - bearerAuth: []
      responses:
        '200':
